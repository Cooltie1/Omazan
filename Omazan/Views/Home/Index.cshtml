@{
    ViewData["Title"] = "Home Page";
}

@model BooksViewModel

@foreach (Books book in Model.books)
{
    <h3>@book.Title</h3>
}

<div page-model="@Model.pageInfo" page-action="Index" page-classes-enabled="true"
        page-class="btn" page-class-normal="btn-outline-dark" page-class-selected="btn-dark" 
        class="btn-group pull-right m-1">
</div>
